<h1>Add Company</h1>
<%= form_with url: companies_path do |form| %>
 <%= form.label :name %>
 <%= form.text_field :name %>
 <%= form.label :office %>
 <%= form.select :office, options_from_collection_for_select(@available_offices,'id', 'info') %>
 <%= form.submit 'Save', class: 'form-submit' %>
<% end %>
